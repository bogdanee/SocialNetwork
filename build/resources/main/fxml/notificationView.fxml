<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane opacity="0.85" prefHeight="100.0" prefWidth="300.0" style="-fx-background-color: black; -fx-border-radius: 5; -fx-border-color: gray;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.NotificationController">
   <children>
      <Button fx:id="buttonOk" layoutX="126.0" layoutY="60.0" mnemonicParsing="false" onAction="#handleOK" style="-fx-background-color: rgba(0,0,0,0);" text="OK" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="labelEvent" layoutX="14.0" layoutY="14.0" text="Label" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
